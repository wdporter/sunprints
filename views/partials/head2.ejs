		"customerPick": "/components/CustomerPick.js",
		"designPick": "/components/DesignPick.js"
	}
}
</script>


	<% if (typeof javascripts != "undefined") { 
		javascripts.forEach(j => {
	%>
	<script src="<%-j %>"></script>
	<% }) 
		} %>
	



