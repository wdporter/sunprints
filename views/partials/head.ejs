			<script src="/javascripts/dataTables.buttons-2.2.3.js"></script>
		<% } else if (title=="Print Designs" || title=="Embroidery Designs" || title=="Transfer Designs" ) { %>
			<link rel=stylesheet href=/stylesheets/printdesign-theme.css>
		<% } else if (title=="Usb" ) { %>
			<link rel=stylesheet href=/stylesheets/usb-theme.css>
		<% } else if (title=="Transfer Designs" ) { %>
			<link rel=stylesheet href="/stylesheets/transfer-theme.css">
		<% } else if (title=="Transfer Names" ) { %>
			<link rel=stylesheet href="/stylesheets/transfername-theme.css">
		<% } else if (title=="Purchasing" ) { %>
			<link rel=stylesheet href=/stylesheets/fixedHeader.dataTables.min.css>
			<link rel=stylesheet href="/stylesheets/purchasing-theme.css">
		<% } else if (title=="Audit Log" ) { %>
